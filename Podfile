# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'RestaurantListing' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for RestaurantListing
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'Alamofire'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'AlamofireImage'
  pod 'SwiftyJSON', '~> 4.0'
  target 'RestaurantListingTests' do
    inherit! :search_paths
    # Pods for testing
    	pod 'RxSwift'
	    pod 'RxCocoa'  
  end

  target 'RestaurantListingUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
